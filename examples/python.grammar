STATEMENTS -> STATEMENTS STATEMENT | STATEMENT
STATEMENT -> SIMPLE_STMT | COMPOUND_STMT | COMMENTS
STATEMENTS_INDENTED -> STATEMENTS_INDENTED indentation STATEMENT | indentation STATEMENT

COMMENTS -> line_comment | multiline_comment
SIMPLE_STMT -> ASSIGNMENT | EXPRESSION
COMPOUND_STMT -> IF_STMT

ASSIGNMENT -> id atr EXPRESSION
EXPRESSION -> EXPRESSION operator VAL | EXPRESSION comparator VAL | VAL
VAL -> id | number | string | TUPLE_LIKE | FUNCTION_CALL | DATA_INDEXING | CONDITIONAL

FUNCTION_CALL -> id TUPLE_LIKE
DATA_INDEXING -> id open_square_bracket COMMA_VALUES_MULT close_square_bracket

TUPLE_LIKE -> open_bracket COMMA_VALUES close_bracket
LIST_LIKE -> open_square_bracket COMMA_VALUES close_square_bracket
SET_LIKE -> open_curly_bracket COMMA_VALUES close_curly_bracket

COMMA_VALUES -> COMMA_VALUES_MULT | &
COMMA_VALUES_MULT -> COMMA_VALUES_MULT comma EXPRESSION | EXPRESSION

IF_STMT -> if EXPRESSION colon STATEMENTS_INDENTED
